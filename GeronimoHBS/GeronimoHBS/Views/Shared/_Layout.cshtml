<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Geronimo - @ViewBag.Title</title>
    @*@Styles.Render("~/Content/css")*@
    @*@Scripts.Render("~/bundles/modernizr")*@
    @Styles.Render("~/Content/Geronimo-css/geronimo.css")


    <link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
    <script src="~/Scripts/Geronimo-scripts/sidePanelNavigation.js"></script>
</head>
<body>

    <div class="sideNav" id="sidePanelNavigation">
        <a href="javascript:void(0)" class="closebtn" onclick="hideNav()">&times;</a>
        <div class="sideNav-content">
            @Html.ActionLink("Home", "Index", "Home")
            @Html.ActionLink("About", "About", "Home")
            @Html.ActionLink("Contact", "Contact", "Home")
        </div>
    </div>

    <span style="font-size:30px;cursor:pointer" onclick="showNav()">&#9776;</span>

    @Html.Partial("_LoginPartial")

  



    <div class="container body-content">
        @RenderBody()

        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
