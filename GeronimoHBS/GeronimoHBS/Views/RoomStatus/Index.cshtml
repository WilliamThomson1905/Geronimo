@model IEnumerable<GeronimoHBS.Models.RoomStatus>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RoomStatusName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RoomStatusName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.RoomStatusID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.RoomStatusID })
        </td>
    </tr>
}

</table>
